{
  "name": "bytic/migrations",
  "license": "MIT",
  "description": "Bytic migrations",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Migrations\\": "src"
    },
    "files": [
      "src/Utility/general.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Migrations\\Tests\\": "tests/src",
      "ByTIC\\Migrations\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "bytic/dotenv": "^4.0|^5.0",
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/console": "^0.9|^1.0",
    "bytic/config": "^0.9|^1.0",
    "robmorgan/phinx": "^0.12",
    "odan/phinx-migrations-generator": "^5.0"
  },
  "require-dev": {
    "bytic/deployer-recipies": "^6.0",
    "bytic/phpqatools": "^1.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
